{
  "mcpProxy": {
    "baseURL": "http://localhost",
    "addr": ":8080",
    "name": "MCP Proxy with Serena",
    "version": "1.0.0",
    "type": "streamable-http",
    "options": {
      "lazyLoad": true
    }
  },
  "mcpServers": {
    "serena": {
      "transportType": "stdio",
      "command": "uvx",
      "args": [
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code"
      ],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    }
  }
}
