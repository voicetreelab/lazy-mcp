{
  "mcpProxy": {
    "baseURL": "http://localhost:9090",
    "addr": ":9090",
    "name": "MCP Proxy with Lazy Loading",
    "version": "1.0.0",
    "type": "sse",
    "options": {
      "lazyLoad": true,
      "logEnabled": true
    }
  },
  "mcpServers": {
    "everything": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-everything"
      ]
    }
  }
}
